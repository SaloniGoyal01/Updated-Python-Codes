   "source": [
    "#   WAP to find the factorial of first n no.(Using For loop)\n",
    "\n",
    "n = 5\n",
    "fact = 1\n",
    "for i in range(1, n+1):\n",
    "    fact*=i\n",
    "print(fact)"
   ]
